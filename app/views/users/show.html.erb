<% provide(:title, @user.name) %>
<div class="container">
  <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12">
    <h1>
      <%= gravatar_for @user %>
      <%= @user.name %>
    </h1>
  </div>
  <div class="col-lg-8 col-md-8 col-sm-6 col-xs-12">
    <%= @user.name %> <%= @user.email %>
  </div>
</div>
